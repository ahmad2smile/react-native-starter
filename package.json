{
	"name": "scheduling",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start":
			"concurrently 'npm run configure' 'npm run depRunner' 'npm run iosRunner'",
		"configure": "node configure.js",
		"depRunner": "node node_modules/react-native/local-cli/cli.js start",
		"iosRunner": "react-native run-ios",
		"clean":
			"rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
		"test": "jest"
	},
	"dependencies": {
		"axios": "^0.17.1",
		"prop-types": "^15.6.0",
		"react": "16.0.0",
		"react-native": "0.51.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-persist": "^5.4.0",
		"redux-saga": "^0.16.0"
	},
	"devDependencies": {
		"babel-eslint": "^8.0.3",
		"babel-jest": "21.2.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-inline-environment-variables": "^0.2.0",
		"babel-preset-react-native": "4.0.0",
		"concurrently": "^3.5.1",
		"eslint": "^4.13.0",
		"eslint-config-standard": "^10.2.1",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-node": "^5.2.1",
		"eslint-plugin-prettier": "^2.3.1",
		"eslint-plugin-promise": "^3.6.0",
		"eslint-plugin-react": "^7.5.1",
		"eslint-plugin-standard": "^3.0.1",
		"jest": "21.2.1",
		"prettier": "^1.9.2",
		"react-test-renderer": "16.0.0",
		"remote-redux-devtools": "^0.5.12"
	},
	"jest": {
		"preset": "react-native"
	}
}
